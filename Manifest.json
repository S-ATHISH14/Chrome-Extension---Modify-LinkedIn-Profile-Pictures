{
    "manifest_version": 3,
    "name": "Modify Linkedin Profile Pictures",
    "version": "1.0",
    "description": "A Chrome extension that changes the profile pictures of all posts on the LinkedIn homepage""
    ,
    "permissions": [
      "activeTab",
      "declarativeContent"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "Red_Kitten_01.jpg",
          "assets/client-cc99106f.js"
        ],
        "matches": ["<all_urls>"]
      }
    ]
  }
